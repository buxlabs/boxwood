{
  "name": "boxwood",
  "version": "0.60.4",
  "description": "Compile HTML templates into JS",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "test": "ava '**/*.spec.js'",
    "coverage": "nyc npm test",
    "benchmark": "ava test/benchmark/index.js --verbose",
    "watch": "npm test -- --watch",
    "prepush": "npm run lint && npm test"
  },
  "ava": {
    "files": [
      "test/spec/**/*.js",
      "**/*.spec.js"
    ]
  },
  "engines": {
    "node": ">= 16.13.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buxlabs/boxwood.git"
  },
  "keywords": [
    "html",
    "engine",
    "javascript"
  ],
  "author": {
    "name": "Emil Ajdyna",
    "email": "emil@ajdyna.com",
    "url": "https://buxlabs.pl/en"
  },
  "contributors": [
    {
      "name": "Przemys≈Çaw Konieczniak",
      "email": "przemyslaw.konieczniak@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/buxlabs/boxwood/issues"
  },
  "homepage": "https://github.com/buxlabs/boxwood#readme",
  "devDependencies": {
    "ava": "^4.3.3",
    "benchmark": "2.1.4",
    "express": "^4.18.1",
    "handlebars": "^4.7.7",
    "jsdom": "^20.0.0",
    "lodash.template": "4.5.0",
    "mustache": "^4.2.0",
    "nyc": "15.1.0",
    "standard": "^17.0.0",
    "underscore": "^1.13.4"
  },
  "standard": {
    "ignore": [
      "test/fixtures/**/*",
      "test/spec/**/*",
      "src/**/*.spec.js"
    ]
  }
}
