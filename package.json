{
  "name": "boxwood",
  "version": "0.78.0",
  "description": "Compile HTML templates into JS",
  "main": "index.js",
  "scripts": {
    "test": "node --test --test-reporter=dot",
    "test:debug": "node --test --test-reporter=spec",
    "coverage": "c8 npm test",
    "benchmark": "node --test benchmark/index.js",
    "watch": "npm test -- --watch",
    "prepush": "npm test"
  },
  "ava": {
    "files": [
      "test/spec/**/*.js",
      "**/*.spec.js"
    ]
  },
  "engines": {
    "node": ">= 20.11.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buxlabs/boxwood.git"
  },
  "keywords": [
    "html",
    "engine",
    "javascript"
  ],
  "author": {
    "name": "Emil Ajdyna",
    "email": "emil@ajdyna.com",
    "url": "https://buxlabs.pl/en"
  },
  "contributors": [
    {
      "name": "Przemys≈Çaw Konieczniak",
      "email": "przemyslaw.konieczniak@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/buxlabs/boxwood/issues"
  },
  "homepage": "https://github.com/buxlabs/boxwood#readme",
  "devDependencies": {
    "benchmark": "2.1.4",
    "c8": "^9.1.0",
    "express": "^4.19.2",
    "handlebars": "^4.7.8",
    "jsdom": "^24.0.0",
    "mustache": "^4.2.0",
    "underscore": "^1.13.6"
  },
  "standard": {
    "ignore": [
      "test/fixtures/**/*",
      "test/spec/**/*",
      "src/**/*.spec.js"
    ]
  },
  "dependencies": {
    "css-tree": "^2.3.1",
    "string-hash": "^1.1.3"
  },
  "prettier": {
    "semi": false
  }
}
