{
  "name": "boxwood",
  "version": "0.60.3",
  "description": "Compile HTML templates into JS",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "test": "ava 'test/spec/**/*.spec.js' 'src/**/*.spec.js' --no-worker-threads",
    "coverage": "nyc npm test",
    "benchmark": "ava test/benchmark.spec.js --verbose",
    "watch": "npm test -- --watch",
    "prepush": "npm run lint && npm test",
    "typecheck": "tsc --checkJs index.js --noEmit --esModuleInterop --target es5"
  },
  "ava": {
    "files": [
      "test/spec/**/*.js",
      "**/*.spec.js"
    ]
  },
  "engines": {
    "node": ">= 16.13.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buxlabs/boxwood.git"
  },
  "keywords": [
    "html",
    "engine",
    "javascript"
  ],
  "author": {
    "name": "Emil Ajdyna",
    "email": "emil@ajdyna.com",
    "url": "https://buxlabs.pl/en"
  },
  "contributors": [
    {
      "name": "Przemys≈Çaw Konieczniak",
      "email": "przemyslaw.konieczniak@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/buxlabs/boxwood/issues"
  },
  "homepage": "https://github.com/buxlabs/boxwood#readme",
  "dependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "abstract-syntax-tree": "^2.20.5",
    "ansi-colors": "^4.1.1",
    "axios": "^0.25.0",
    "axios-extensions": "3.1.3",
    "css-tree": "^1.1.3",
    "csso": "^5.0.2",
    "esbuild": "^0.14.20",
    "himalaya": "1.1.0",
    "himalaya-walk": "1.0.0",
    "html-lexer": "^0.4.1",
    "html-minifier": "4.0.0",
    "memoizee": "0.4.15",
    "negate-sentence": "0.2.0",
    "path-to-regexp": "6.2.0",
    "pure-conditions": "1.2.1",
    "pure-utilities": "^1.2.4",
    "rollup": "^2.67.1",
    "rollup-plugin-includepaths": "0.2.4",
    "string-hash": "1.1.3",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "ava": "^4.0.1",
    "benchmark": "2.1.4",
    "browser-env": "3.3.0",
    "express": "^4.17.2",
    "handlebars": "^4.7.7",
    "lodash.template": "4.5.0",
    "mustache": "^4.2.0",
    "nyc": "15.1.0",
    "puppeteer": "^13.2.0",
    "standard": "^16.0.4",
    "typescript": "^4.5.5",
    "underscore": "^1.13.2"
  },
  "standard": {
    "ignore": [
      "test/fixtures/**/*",
      "test/spec/**/*",
      "src/**/*.spec.js"
    ]
  }
}
