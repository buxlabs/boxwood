{
  "name": "boxwood",
  "version": "2.4.2",
  "description": "Compile HTML templates into JS",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": "./index.js",
    "./adapters/express": "./adapters/express/index.js",
    "./ui": "./ui/index.js"
  },
  "scripts": {
    "test": "node --test --test-reporter=dot \"**/*.test.js\" \"**/*.spec.js\"",
    "test:debug": "node --test --test-reporter=spec \"**/*.test.js\" \"**/*.spec.js\"",
    "coverage": "c8 npm test",
    "benchmark": "node --test benchmark/index.js",
    "watch": "npm test -- --watch",
    "prepush": "npm test"
  },
  "engines": {
    "node": ">= 24.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buxlabs/boxwood.git"
  },
  "keywords": [
    "html",
    "engine",
    "javascript"
  ],
  "author": {
    "name": "Emil Ajdyna",
    "email": "emil@ajdyna.com",
    "url": "https://buxlabs.pl/en"
  },
  "contributors": [
    {
      "name": "Przemys≈Çaw Konieczniak",
      "email": "przemyslaw.konieczniak@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/buxlabs/boxwood/issues"
  },
  "homepage": "https://github.com/buxlabs/boxwood#readme",
  "devDependencies": {
    "benchmark": "2.1.4",
    "c8": "^10.1.3",
    "express": "^5.1.0",
    "handlebars": "^4.7.8",
    "jsdom": "^26.1.0",
    "mustache": "^4.2.0",
    "underscore": "^1.13.7"
  },
  "standard": {
    "ignore": [
      "test/fixtures/**/*",
      "test/spec/**/*",
      "src/**/*.spec.js"
    ]
  },
  "dependencies": {
    "css-tree": "^3.1.0"
  },
  "prettier": {
    "semi": false
  }
}
