{
  "name": "boxwood",
  "version": "0.50.4",
  "description": "Compile HTML templates into JS",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "test": "ava 'test/spec/**/*.spec.js' 'src/**/*.spec.js'",
    "coverage": "nyc npm test",
    "benchmark": "ava test/benchmark.spec.js --verbose",
    "watch": "npm test -- --watch",
    "build": "node website/build.js",
    "prepush": "npm run lint && npm test",
    "typecheck": "tsc --checkJs index.js --noEmit --esModuleInterop --target es5"
  },
  "ava": {
    "files": [
      "test/spec/**/*.js",
      "**/*.spec.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test && npm run lint"
    }
  },
  "engines": {
    "node": ">= 14.15.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buxlabs/boxwood.git"
  },
  "keywords": [
    "html",
    "engine",
    "javascript"
  ],
  "author": {
    "name": "Emil Ajdyna",
    "email": "emil@ajdyna.com",
    "url": "https://buxlabs.pl/en"
  },
  "contributors": [
    {
      "name": "Przemys≈Çaw Konieczniak",
      "email": "przemyslaw.konieczniak@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/buxlabs/boxwood/issues"
  },
  "homepage": "https://github.com/buxlabs/boxwood#readme",
  "dependencies": {
    "abstract-syntax-tree": "2.15.2",
    "axios": "0.21.1",
    "axios-extensions": "3.1.3",
    "css-tree": "1.1.2",
    "esbuild": "0.8.31",
    "himalaya": "1.1.0",
    "himalaya-walk": "1.0.0",
    "html-lexer": "0.4.0",
    "image-size": "0.9.3",
    "memoizee": "0.4.15",
    "negate-sentence": "0.2.0",
    "normalize-newline": "3.0.0",
    "path-to-regexp": "6.2.0",
    "pure-conditions": "1.2.1",
    "pure-utilities": "1.2.2",
    "string-hash": "1.1.3",
    "words-to-numbers": "1.5.1",
    "yaml": "1.10.0"
  },
  "devDependencies": {
    "ava": "3.15.0",
    "benchmark": "2.1.4",
    "browser-env": "3.3.0",
    "express": "4.17.1",
    "handlebars": "4.7.6",
    "husky": "4.3.7",
    "lodash.template": "4.5.0",
    "mustache": "4.1.0",
    "nyc": "15.1.0",
    "puppeteer": "5.5.0",
    "standard": "16.0.3",
    "typescript": "4.1.3",
    "underscore": "1.12.0"
  },
  "standard": {
    "ignore": [
      "test/fixtures/**/*",
      "test/spec/**/*",
      "src/**/*.spec.js"
    ]
  },
  "optionalDependencies": {
    "sharp": "0.26.2"
  }
}
