{
  "name": "boxwood",
  "version": "0.40.0",
  "description": "Compile HTML templates into JS",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "test": "ava 'test/spec/**/*.spec.js' 'src/**/*.spec.js'",
    "coverage": "nyc npm test",
    "benchmark": "ava test/benchmark.spec.js --verbose",
    "watch": "npm test -- --watch",
    "build": "node website/build.js"
  },
  "ava": {
    "files": [
      "test/spec/**/*.js",
      "**/*.spec.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test && npm run lint"
    }
  },
  "engines": {
    "node": ">= 10.15.1",
    "npm": ">= 6.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buxlabs/boxwood.git"
  },
  "keywords": [
    "html",
    "engine",
    "javascript"
  ],
  "author": {
    "name": "Emil Ajdyna",
    "email": "emil@ajdyna.com",
    "url": "https://buxlabs.pl/en"
  },
  "contributors": [
    {
      "name": "Przemys≈Çaw Konieczniak",
      "email": "przemyslaw.konieczniak@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/buxlabs/boxwood/issues"
  },
  "homepage": "https://github.com/buxlabs/boxwood#readme",
  "dependencies": {
    "abstract-syntax-tree": "^2.8.1",
    "astoptech": "^1.0.6",
    "axios": "^0.19.2",
    "axios-extensions": "^3.1.3",
    "css-tree": "^1.0.0-alpha.39",
    "himalaya": "^1.1.0",
    "himalaya-walk": "^1.0.0",
    "html-lexer": "^0.3.0",
    "image-size": "^0.8.3",
    "memoizee": "^0.4.14",
    "negate-sentence": "^0.2.0",
    "normalize-newline": "^3.0.0",
    "path-to-regexp": "^6.1.0",
    "pure-conditions": "^1.1.0",
    "pure-utilities": "^1.2.0",
    "rollup": "^2.18.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-includepaths": "^0.2.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "string-hash": "^1.1.3",
    "words-to-numbers": "^1.5.1",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "ava": "^3.9.0",
    "benchmark": "^2.1.4",
    "express": "^4.17.1",
    "handlebars": "^4.7.6",
    "husky": "^4.2.5",
    "lodash.template": "^4.5.0",
    "mustache": "^4.0.1",
    "nyc": "^15.1.0",
    "puppeteer": "^3.3.0",
    "standard": "^14.3.4",
    "underscore": "^1.10.2"
  },
  "standard": {
    "ignore": [
      "test/fixtures/**/*",
      "test/spec/**/*",
      "src/**/*.spec.js"
    ]
  }
}
